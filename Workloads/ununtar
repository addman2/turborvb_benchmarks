#!/usr/bin/env bash

./H-300el-nopseudo-pbc/clean
./H-686el-nopseudo-pbc/clean
./H-1024el-nopseudo-pbc/clean
./P-1280el-pseudo-pbc/clean
./CuO-26el-pseudo-open/clean

tar czf H-300el-nopseudo-pbc.tar.gz H-300el-nopseudo-pbc
tar czf H-686el-nopseudo-pbc.tar.gz H-686el-nopseudo-pbc
tar czf H-1024el-nopseudo-pbc.tar.gz H-1024el-nopseudo-pbc
tar czf P-1280el-pseudo-pbc.tar.gz P-1280el-pseudo-pbc
tar czf CuO-26el-pseudo-open.tar.gz CuO-26el-pseudo-open

rm -rf H-300el-nopseudo-pbc
rm -rf H-686el-nopseudo-pbc
rm -rf H-1024el-nopseudo-pbc
rm -rf P-1280el-pseudo-pbc
rm -rf CuO-26el-pseudo-open
